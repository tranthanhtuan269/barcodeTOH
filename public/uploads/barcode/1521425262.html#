        // Track payment options desktop
        if ($('form#payment_information_form').length > 0) {
            trackSelectedPaymentOption(true);
        }

        $('form#payment_information_form').submit(function () {
            trackSelectedPaymentOption();
        });

        // Track payment options mobile theme
        if ($('form.l-payment-systems__cell_expanded').find('.l-payment-systems__tab').length > 0) {
            trackMobilePaymentOption(true);
        }

        $('form.l-payment-systems__cell_expanded').submit(function () {
            trackMobilePaymentOption();
        });

        $('#google-login-button, .tracking-google-login-button').on('click', function () {
            setECommerceCheckoutOption('Google');
        });
        $('#facebook-login-button, .tracking-facebook-login-button').on('click', function () {
            setECommerceCheckoutOption('Facebook');
        });

        $('#email_login_form').submit(function () {
            var loginOption = $('#EmailLoginForm_isGuestCheckout:checked').val() == 1 ? 'Guest' : 'Return user';
            setECommerceCheckoutOption(loginOption);
        });

                if ($('form.l-payment-systems__cell_expanded').find('.l-payment-systems__tab').length > 0) {
            var paymentType = getPaymentValue($('form.l-payment-systems__cell_expanded').find('.l-payment-systems__tab'));
            setECommerceCheckoutOption(paymentType, true)
        }

        $('.l-payment-systems__tab').on('click', function(e) {
            var parentFormDisable = $(this).closest('form').attr('data-cause-disable');
            if(!$(this).closest(".l-payment-systems_active").length && typeof parentFormDisable == "undefined") {
                setECommerceCheckoutOption(getPaymentValue(this));
            }

        });
        
        dataLayer.push(enhanceECommerceEvent);

        function trackDeliveryOptions(setObject) {
            var options = [];
            $('.input-shipping-fee-delivery:checked').each(function () {
                if (options.indexOf($(this).val()) == -1) {
                    options.push($(this).val());
                }
            });
            if (options.length > 0) {
                setECommerceCheckoutOption(options.join('/'), setObject)
            }
        }

        function trackSelectedPaymentOption(setObject) {
            var activeTabIdx = $('#ActiveTabIndexValue').val();
            var selectedPayment = ($('.payment-options-list').find('li').eq(activeTabIdx).find('a').attr('id'));
            if (typeof  selectedPayment != 'undefined') {
                var methodChunk = selectedPayment.split('_');
                if (methodChunk[1] != 'undefined') {
                    setECommerceCheckoutOption(methodChunk[1], setObject);
                }
            }

        }

        function setECommerceCheckoutOption(option, setObject) {
            if (typeof option == 'undefined') {
                return;
            }
            if (setObject == true && typeof enhanceECommerceEvent != 'undefined') {
                enhanceECommerceEvent.ecommerce.checkout.actionField.option = option;
                return true;
            }
            var step = enhanceECommerceEvent.ecommerce.checkout.actionField.step;
            dataLayer.push({
                'event': 'checkoutOption',
                'ecommerce': {
                    'checkout_option': {
                        'actionField': {'step': step, 'option': option.trim()}
                    }
                }
            });
        }

        function getPaymentValue(obj)
        {
            var paymentValue = $(obj).find('.l-payment-systems__radio:input').val();
            if (paymentValue == 'Cybersource' || paymentValue == "undefined") {
                paymentValue = $(obj).find('.l-payment-systems__radio:input').parent().find('.l-payment-systems__icon').attr('title');
            }
            return paymentValue;
        }


        
    });


</script>
<script type="text/javascript">
    (function() {
        function loadScript (scriptpath){
            var s=document.getElementsByTagName('script')[0];
            var ss=document.createElement('script');
            ss.type='text/javascript';
            ss.async=true;
            ss.src= scriptpath;
            s.parentNode.insertBefore(ss,s);
        }
        loadScript('https://vn-live-01.slatic.net/lzdtracker/lzdtracker.js?v=2a4a781');
        loadScript('https://vn-live-02.slatic.net/lzdtracker/resource_performance.js?v=2a4a781');
    })();
</script><!-- A+ gracking start -->
<script type="text/javascript">
    (function () {
        function APlusTracking() {
            this.cfg = {
                scriptAPISrc: '//assets.alicdn.com/g/alilog/mlog/aplus_int.js',
                spmB: 'prod',
                venture: 'VN',
                smpVenture: 'a2o4n',
                additionalParams: 'clog=o&aplus=&sidx=aplusSidx&ckx=aplusCkx&_p_ispdp=1&_p_item=OE680FAAA38Z5PVNAMZ-5693160&_p_slr=25320&anonymous_id=2736958ef44c72883519486e32ec335e&_p_client_type=web&_p_typ=pdp'
            };
        }

        APlusTracking.prototype = {
            init: function () {
                window.goldlog_queue = window.goldlog_queue || [];
                this.insertSPMIds();
                this.bindEvents();
                this.initScript();
            },
            bindEvents: function () {
                $(window).on('aPlusEventCart', function (e, sku) {
                    window.goldlog_queue.push({
                        action: 'goldlog.record',
                        arguments: ['/lazada.add_to_cart.cart_add', 'CLK', 'sku_added=' + sku, 'H1508277582']
                    });
                });
                var aPlustracking = this;
                window.addEventListener('regflowstepchanged', function(evt) {
                    if (evt.data.pageType) {
                        aPlustracking.insertSPMIds(evt.data.pageType);
                        window.goldlog_queue.push({
                            action: 'goldlog.sendPV',
                            arguments: [{checksum: aPlustracking.makeChecksum(location.href)}]
                        });
                    }
                });
            },
            initScript: function () {
                var el = document.createElement('script');
                el.setAttribute('exparams', this.cfg.additionalParams);
                el.id = 'beacon-aplus';
                el.src = this.cfg.scriptAPISrc;
                document.body.appendChild(el);
            },
            insertSPMIds: function (spmB) {
                if (typeof spmB === 'undefined') {
                    spmB = this.cfg.spmB
                }

                window.goldlog_queue.push({
                    action: 'goldlog.setPageSPM',
                    arguments: [this.getPageSpmByVenture(this.cfg.venture), spmB]
                });
            },
            getPageSpmByVenture: function (venture) {
                //according to https://confluence.lzd.co/display/DS/SPM-ID+a.b.c.d+Implementation+template#html-attribute-value
                if (this.cfg.smpVenture != '') {
                    return this.cfg.smpVenture;
                }

                var values = {
                    ID: 'a2o4j',
                    MY: 'a2o4k',
                    PH: 'a2o4l',
                    SG: 'a2o42',
                    TH: 'a2o4m',
                    VN: 'a2o4n'
                };

                return values[venture];
            },
            makeChecksum: function (s) {
                s = (s || '').split('#')[0].split('?')[0];
                var len = s.length;
                var hash = function (s) {
                    var i;
                    var l = s.length;
                    var key = 0;

                    for (i = 0; i < l; i++) {
                        key = key * 31 + s.charCodeAt(i);
                    }
                    return key;
                };

                return len ? hash(len + '#' + s.charCodeAt(len - 1)) : -1;
            }
        };

        new APlusTracking().init();

    })();
</script>
<!-- A+ gracking end --></body>
</html>
