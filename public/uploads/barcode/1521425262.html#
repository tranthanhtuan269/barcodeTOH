                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') c = c.substring(1);
                    if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
                }
                return "";
            }

            function setCookie(cname, cvalue, exhours) {
                var d = new Date();
                d.setTime(d.getTime() + (exhours * 60 * 60 * 1000));
                var expires = "expires=" + d.toGMTString();
                document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
            }

            var sessionCounter = getCookie('session_counter');
            var utmaValue = getCookie('__utma');
            if (utmaValue != '') {
                var gaSessionCounter = parseInt(utmaValue.split('.').pop());
                if (gaSessionCounter != '' && sessionCounter != gaSessionCounter) {
                    var url = "/tracking/sessioncounter/";
                    var csrfTokenName = 'FORM_TOKEN';
                    var csrfToken = '56966715d8773596bf53a414c3833b06e341f00a';
                    if (typeof(jQuery) != "undefined") {
                        jQuery.ajax({
                            url: url,
                            type: 'post',
                            dataType: 'json',
                            data: {
                                csrfTokenName: csrfTokenName,
                                csrfToken: csrfToken
                            }
                        });
                    }
                    else {
                        var http = new XMLHttpRequest();
                        var params = "csrfTokenName=" + csrfTokenName + "&csrfToken=" + csrfToken;
                        http.open("POST", url, true);
                        http.setRequestHeader("Content-type", "application/x-www-form-urlencoded; charset=UTF-8");
                        http.setRequestHeader("Content-length", params.length);
                        http.setRequestHeader("X-Requested-With", "XMLHttpRequest");
                        http.setRequestHeader("Connection", "close");
                        http.send(params);
                    }
                }
                setCookie('session_counter', gaSessionCounter, 1);
            }
        }]);

        
                        _gaq.push(['_setCustomVar', 10, 'Visit PDP', '60 - 1']);
        
        if (typeof pageType == 'undefined') {
                        _gaq.push(['_trackPageview']);
                    } else {
            _gaq.push(['_trackPageview', '/' + pageType]);
        }
    }

    triggerGaTracking();

    window.addEventListener('regflowstepchanged', function(evt) {
        if (evt.data.pageType) {
            triggerGaTracking(evt.data.pageType);
        }
    });

    function loadScript(scriptpath, callback) {
        var s=document.getElementsByTagName('script')[0];
        var ss=document.createElement('script');
        ss.type='text/javascript';
        ss.async=true;
        ss.onload = function() {
            if (typeof callback == 'function')
            {
                callback();
            }
        };
        ss.src= scriptpath;
        s.parentNode.insertBefore(ss,s);
    }
    loadScript('//stats.g.doubleclick.net/dc.js');

</script>

<script type="text/javascript">
    if (window.addEventListener) {
        window.addEventListener('error', trackJavaScriptError, false);
    } else if (window.attachEvent) {
        window.attachEvent('onerror', trackJavaScriptError);
    } else {
        window.onerror = trackJavaScriptError;
    }

    function trackJavaScriptError(e) {
        var ie = window.event || {},
            errMsg = e.message || ie.errorMessage;
        var filename = (e.filename || ie.errorUrl);
        if ('undefined' == filename || !filename) {
            filename = document.URL;
        }
        var errSrc = filename + ': ' + (e.lineno || ie.errorLine);
        _gaq.push(['_trackEvent', 'JavaScript Error', errMsg, errSrc, 0, true]);
    }
</script><!-- Google Analytics end -->

