$(document).ready(function() {
    

    var lastPriceString = $('#special_price_box').text().replace('VND', '').replace(/\./g, '').trim();
    var lastPriceInt = parseInt(lastPriceString);
var listType = [];
$(".delivery-option-st").each(function(){
	listType.push($(this).data("type"));
});

    try {
         /* Deatail page */
if($('.product__detail-badge').hasClass('vta_b') && ($("html").attr("lang") == "vi")) {
	$("#productDetailBadge").after('<div style="border: 1px solid rgba(85, 120, 171, 0.61);padding: 7px 10px;box-sizing:border-box;">Khi mua sản phẩm này, Nhà bán hàng VienthongA sẽ gọi điện thoại tư vấn trực tiếp và quý khách sẽ đến nhận hàng ngay tại các cửa hàng VienthongA</div>');
} else if($('.product__detail-badge').hasClass('vta_b') && ($("html").attr("lang") == "en")) {
	$("#productDetailBadge").after('<div style="border: 1px solid rgba(85, 120, 171, 0.61);padding: 7px 10px;box-sizing:border-box;">Khi mua sản phẩm này, Nhà bán hàng VienthongA sẽ gọi điện thoại tư vấn trực tiếp và quý khách sẽ đến nhận hàng ngay tại các cửa hàng VienthongA.</div>');
}

