<script>
  function stackclose() {
    document.getElementById("stack-mobile-container").remove();
    try{
      localStorage.setItem("popupclose", (new Date).getTime())
    } catch(e){console.log(e);}
  }

  function stackclose() {
    document.getElementById("stack-mobile-container1").remove();
    try{
      localStorage.setItem("popupclose1", (new Date).getTime())
    } catch(e){console.log(e);}
  }
</script>
