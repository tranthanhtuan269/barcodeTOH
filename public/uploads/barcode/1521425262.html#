    {{#list}}
    <li class="location-options__item">
        <div class="location-options__cell">
            <input
                class="location-options__input-radio"
                id="location-option-{{id}}"
                type="radio"
                name="option-location-select-{{type}}"
                value="{{id}}"
                data-text="{{text}}"
            >
        </div>
        <div class="location-options__cell">
            <label class="location-options__label" for="location-option-{{id}}">
                {{text}}
            </label>
        </div>
    </li>
    {{/list}}
</script>
<script type="text/template" id="template-delivery-type">
    {{#list}}
    <li class="delivery-type__item">
        <div class="delivery-type__row">
            <div class="delivery-type__icon-wrapper">
                <i class="delivery-type__icon icon-delivery-{{icon_class_suffix}}"></i>
            </div>
            <div class="delivery-type__content">
                <div class="delivery-type__label">{{shortTitle}}:</div>
                <div class="delivery-type__time">{{deliveryDayRange}}</div>
            </div>
            <div class="delivery-type__price">{{priceOnly}}</div>
        </div>

        {{#additionalInfo}}
        <div class="delivery-type__row">
            <div class="delivery-type__shipping-promotion-text">
                {{{additionalInfo}}}
            </div>
        </div>
        {{/additionalInfo}}
    </li>
    {{/list}}

    {{^list}}
    <li class="delivery-type__item">
        {{#message}}
            <div class="delivery-type__error">
                {{{message}}}
            </div>
        {{/message}}
    </li>
    {{/list}}
</script>
<script type="text/template" id="template-delivery-options-tooltip">

