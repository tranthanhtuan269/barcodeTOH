<script type="text/javascript"> $(document).ready(function() { 
/* Fix bug trả hàng */
var getURL = window.location.pathname;
if (getURL.indexOf("customer") != -1) {
    if ($('.revokeItem input:last-child').attr('class') != "itemCheckbox") {
        $('.requestRevokeList .pbs').html('Bạn không thể thực hiện đổi trả hàng')
        $('.requestRevokeList > .ptm').html('Các sản phẩm trong giỏ hàng')
    }
}

/* Remove content of merchant info */
$(document).ready(function() {
    $('.merchant_info').appendTo('.merchant_product');
});

/* Change from OFF to - in catalog page */
$(document).ready(function() {
    $('.product-card__sale').each(function(idx, el) {
        if ($(this).text().indexOf('OFF') != -1) {
            $(this).text("- " + $(this).text().replace('OFF', '').trim());
        }
    });
});


$('.c-product-card.tbd01 .c-product-card__price').each(function() {
    $(this).closest(".c-product-card").find(".c-quick-buy").before("<div class='text-sticker'>Mua nhiều giảm nhiều</div>")
});
$('.c-product-card.tbd11 .c-product-card__price').each(function() {
    $(this).closest(".c-product-card").find(".c-quick-buy").before("<div class='text-sticker'>Mua một tặng một</div>")
});
