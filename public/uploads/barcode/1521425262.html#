    <div class="tooltip-launcher" data-tooltiplauncher="delivery-info">
        <i class="tooltip-launcher__icon"></i>
    </div>
    <div class="popup-tooltips" data-tooltip="delivery-info">
        <div class="popup-tooltips__overlay"></div>
        <div class="popup-tooltips__body">
            <div class="popup-tooltips__titles">
                <div class="popup-tooltips__main-title">Chọn địa chỉ giao hàng</div>
                <div class="popup-tooltips__subtitle"></div>
            </div>
            <div class="popup-tooltips__content-wrapper">
                <div class="popup-tooltips__content">
                    <div class="delivery-options-tooltip">
                        {{#tooltipList}}
