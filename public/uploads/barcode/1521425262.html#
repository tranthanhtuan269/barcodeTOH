}

/* Start  lrp10mar */
if($('.product__detail-badge').hasClass('lrp10mar')) {
    if(lastPriceInt > 800000) {
        newPrice = lastPriceInt - 80000;
    }
    else {
        newPrice = lastPriceInt - lastPriceInt * 0.1;
    }
    $('#product-price-box .prod_pricebox_price_final').prev('.mobile-block').remove();
    $('#product-price-box .prod_pricebox_price_final').before('<div class="mobile-block"><div class="dnew-price-all dnew-price-10"><div class="np-left"><div class="np-left-title">Nhập mã</div>   <div class="np-voucher">LRP10MAR</div></div><div class="np-right"><div class="np-left-title">Giảm còn</div><div class="np-prize">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div></div></div>' );
}
/* End lrp10mar Mar22*/

/* Start  vic10t3 */
if($('.product__detail-badge').hasClass('vic10t3')) {
    if(lastPriceInt > 1200000) {
        newPrice = lastPriceInt - 120000;
    }
    else {
        newPrice = lastPriceInt - lastPriceInt * 0.1;
    }
    $('#product-price-box .prod_pricebox_price_final').prev('.mobile-block').remove();
    $('#product-price-box .prod_pricebox_price_final').before('<div class="mobile-block"><div class="dnew-price-all dnew-price-10"><div class="np-left"><div class="np-left-title">Nhập mã</div>   <div class="np-voucher">VIC10T3</div></div><div class="np-right"><div class="np-left-title">Giảm còn</div><div class="np-prize">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div></div></div>' );
}
/* End vic10t3 Mar22*/

/* Start  ibiemar */
if($('.product__detail-badge').hasClass('ibiemar')) {
    newPrice = lastPriceInt - lastPriceInt * 0.1;
    $('#product-price-box .prod_pricebox_price_final').prev('.mobile-block').remove();
    $('#product-price-box .prod_pricebox_price_final').before('<div class="mobile-block"><div class="dnew-price-all dnew-price-10"><div class="np-left"><div class="np-left-title">Nhập mã</div>   <div class="np-voucher">IBIEMAR</div></div><div class="np-right"><div class="np-left-title">Giảm còn</div><div class="np-prize">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div></div></div>' );
}
/* End ibiemar April20 */














/* Start  do15 */
if($('.product__detail-badge').hasClass('do15')) {
    if(lastPriceInt > 666000) {
        newPrice = lastPriceInt - 100000;
    }
    else {
        newPrice = lastPriceInt - lastPriceInt * 0.15;
    }
    $('#product-price-box .prod_pricebox_price_final').prev('.mobile-block').remove();
    $('#product-price-box .prod_pricebox_price_final').before('<div class="mobile-block"><div class="mobile-price">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div>');
}
/* End do15 Mar31*/



    } catch (err) {


    }
});



